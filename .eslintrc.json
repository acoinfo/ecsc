{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": ["error", "never"],
        "quotes": ["error", "single"],
        "indent": ["error", 2],
        "operator-linebreak": ["error"],
        "keyword-spacing": [ "error" ],
        "space-infix-ops": [ "error" ]
    }
}
